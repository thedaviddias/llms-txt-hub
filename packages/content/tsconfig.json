{
  "extends": "../../configs/typescript/base.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": ".",
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./data/*"]
    }
  },
  "include": ["data/**/*"],
  "exclude": ["node_modules", "dist"]
}
